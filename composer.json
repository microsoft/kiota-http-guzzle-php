{
    "name": "microsoft/kiota-http-guzzle",
    "description": "Kiota HTTP Request Adapter implementation",
	"version": "1.3.1",
    "type": "library",
	"repositories": [
		{
			"type": "path",
			"url": "../../abstractions",
			"options": {
				"symlink": false
			}
		}
	],
	"require": {
		"php": "^7.4 | ^8.0",
		"guzzlehttp/guzzle": "^7.4.5",
		"microsoft/kiota-abstractions": "^1.1.0",
		"ext-zlib": "*",
		"ext-json": "*"
	},
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.2"
	},
    "license": "MIT",
    "authors": [
        {
            "name": "Microsoft Graph Client Tooling",
            "email": "graphtooling@service.microsoft.com"
        }
    ],
	"autoload": {
		"psr-4": {
			"Microsoft\\Kiota\\Http\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Microsoft\\Kiota\\Http\\Test\\": "tests/"
		}
	},
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
